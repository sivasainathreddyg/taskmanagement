{
  "_version": "1.76.0",
  "sap.app": {
    "id": "com.taskmanagement.taskmanagement",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.7",
      "toolsId": "0a72e28d-bcc5-491e-a381-d9ba34163a14"
    },
    "dataSources": {
      "mainService": {
        "uri": "/v2/odata/v4/Task-Manager/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "ZTaskManagement-display": {
          "semanticObject": "ZTaskManagement",
          "action": "display",
          "title": "{{ZTaskManagement-display.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.140.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.taskmanagement.taskmanagement.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.taskmanagement.taskmanagement.view",
        "async": true,
        "viewPath": "com.taskmanagement.taskmanagement.view"
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": ":?query:",
          "target": [
            "TargetView1"
          ]
        },
        {
          "name": "ChangeRequest",
          "pattern": "ChangeRequest",
          "target": [
            "ChangeRequest"
          ]
        },
        {
          "name": "Employeemanagement",
          "pattern": "Employeemanagement",
          "target": [
            "Employeemanagement"
          ]
        },
        {
          "name": "Incidents",
          "pattern": "Incidents",
          "target": [
            "Incidents"
          ]
        },
        {
          "name": "PRCDefects",
          "pattern": "PRCDefects",
          "target": [
            "PRCDefects"
          ]
        },
        {
          "name": "Projects",
          "pattern": "Projects",
          "target": [
            "Projects"
          ]
        }
      ],
      "targets": {
        "TargetView1": {
          "id": "View1",
          "name": "View1"
        },
        "ChangeRequest": {
          "id": "ChangeRequest",
          "name": "ChangeRequest"
        },
        "Employeemanagement": {
          "id": "Employeemanagement",
          "name": "Employeemanagement"
        },
        "Incidents": {
          "id": "Incidents",
          "name": "Incidents"
        },
        "PRCDefects": {
          "id": "PRCDefects",
          "name": "PRCDefects"
        },
        "Projects": {
          "id": "Projects",
          "name": "Projects"
        }
      }
    },
    "rootView": {
      "viewName": "com.taskmanagement.taskmanagement.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "comtaskmanagementtaskmanagement"
  }
}
