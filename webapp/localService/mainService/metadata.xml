                <Property Name="updateDate" Type="Edm.DateTimeOffset"/>
                <Property Name="onHoldcheck" Type="Edm.String" MaxLength="1"/>
                <Property Name="vistexcheck" Type="Edm.String" MaxLength="1"/>
            </EntityType>
            <EntityType Name="ProjectManagement">
                <Key>
                    <PropertyRef Name="projectID"/>
                    <PropertyRef Name="number"/>
                </Key>
                <Property Name="projectID" Type="Edm.Int32" Nullable="false"/>
                <Property Name="number" Type="Edm.Int32" Nullable="false"/>
                <Property Name="description" Type="Edm.String"/>
                <Property Name="projectName" Type="Edm.String" MaxLength="100"/>
                <Property Name="plannedStartDate" Type="Edm.DateTime" sap:display-format="Date"/>
                <Property Name="plannedEndDate" Type="Edm.DateTime" sap:display-format="Date"/>
                <Property Name="status" Type="Edm.String" MaxLength="50"/>
                <Property Name="assignedTo" Type="Edm.String" MaxLength="50"/>
                <Property Name="teamSize" Type="Edm.Int32"/>
            </EntityType>
            <EntityType Name="PRCDefects">
                <Key>
                    <PropertyRef Name="id"/>
                </Key>
                <Property Name="id" Type="Edm.Int32" Nullable="false"/>
                <Property Name="title" Type="Edm.String" MaxLength="200"/>
                <Property Name="description" Type="Edm.String"/>
                <Property Name="assignedTo" Type="Edm.String" MaxLength="50"/>
                <Property Name="priority" Type="Edm.String" MaxLength="20"/>
                <Property Name="status" Type="Edm.String" MaxLength="50"/>
                <Property Name="effort" Type="Edm.Int32"/>
                <Property Name="resolution" Type="Edm.String"/>
